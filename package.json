{
  "name": "gatsby-plugin-smartertrack",
  "version": "0.0.9",
  "description": "Adds SmarterTrack by SmarterTools chat widget and who's on tracking scripts.",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir . --ignore test",
    "testBuildBrowser": "node_modules/.bin/webpack --mode development --config webpack/test.browser",
    "testBuildNode": "node_modules/.bin/webpack --mode development --config webpack/test.node",
    "testNodeBundle": "npm run testBuildNode && node_modules/.bin/tape dist/jsdom.environment.js dist/app.bundle.js | node_modules/.bin/tap-spec",
    "test": "NODE_ENV=development node_modules/.bin/tape -r babel-register test/jsdom.environment.js test/unit/{,**/}/*.test.js",
    "test_OLD": "NODE_ENV=development node_modules/.bin/tape --require esm test/jsdom.environment.js test/unit/{,**/}/*.test.js | node_modules/.bin/tap-spec",
    "testStart": "node_modules/.bin/webpack-dev-server --mode development --config webpack/test.browser --open",
    "testem": "node_modules/.bin/testem -f testem.node.json",
    "debug": "NODE_ENV=debug node --nolazy --inspect-brk=5858 node_modules/.bin/tape -r babel-register test/jsdom.environment.js test/unit/{,**/}/*.test.js",
    "debug_OLD": "NODE_ENV=debug node --nolazy --inspect-brk=5858 node_modules/.bin/tape --require esm test/jsdom.environment.js test/unit/{,**/}/*.test.js",
    "devBuild": "NODE_ENV=development babel src --out-dir . --ignore test",
    "devBuild_OLD": "node_modules/.bin/webpack --mode development --config webpack/dev",
    "devWatch": "npm run devBuild -- --watch",
    "devStart": "node_modules/.bin/webpack-dev-server --mode development --config webpack/dev --open",
    "prodBuild": "node_modules/.bin/webpack --mode production --config webpack/prod",
    "lint": "node_modules/.bin/prettier-eslint \"*/**/*.js\" --log-level info",
    "lintFix": "node_modules/.bin/prettier-eslint  \"*/**/*.js\" --write --log-level warn",
    "coverage": "NODE_ENV=development node_modules/nyc/bin/nyc.js npm test",
    "coverReport": "npm run coverage && node_modules/nyc/bin/nyc.js report --reporter=html && ./bin/opn-cli coverage/index.html -- chrome",
    "coveralls": "npm run coverage && node_modules/nyc/bin/nyc.js report --reporter=text-lcov | node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lfurzewaddock/gatsby-plugin-smartertrack.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "SmarterTrack",
    "SmarterTools",
    "chat",
    "tracking",
    "widget",
    "script"
  ],
  "author": "Leo Furze-Waddock <leo@furze-waddock.com>",
  "license": "MIT",
  "private": false,
  "bugs": {
    "url": "https://github.com/lfurzewaddock/gatsby-plugin-smartertrack/issues"
  },
  "homepage": "https://github.com/lfurzewaddock/gatsby-plugin-smartertrack#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "coveralls": "^3.0.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "esm": "^3.0.84",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^11.12.0",
    "minimist": "^1.2.0",
    "nyc": "^11.9.0",
    "opn": "^5.4.0",
    "prettier-eslint-cli": "^4.7.1",
    "tap-spec": "^4.1.2",
    "tape": "^4.9.1",
    "testdouble": "^3.8.1",
    "testem": "^2.12.0",
    "webpack": "^4.20.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "peerDependencies": {
    "gatsby": ">= 2.0.0"
  },
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "prop-types": "^15.6.2"
  }
}
